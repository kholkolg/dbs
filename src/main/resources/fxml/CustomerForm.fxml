                      <VBox.margin>
                        <Insets right="10.0"/>
                     </VBox.margin></TextField>
               </children></VBox>
         </children>
      </HBox>
      <HBox prefHeight="75.0" prefWidth="600.0" spacing="10.0">
         <children>
            <VBox prefHeight="75.0" prefWidth="300.0">
               <children>
                  <Label alignment="CENTER"  prefHeight="25.0" prefWidth="300.0" text="Pnone*" textFill="#871414" />
