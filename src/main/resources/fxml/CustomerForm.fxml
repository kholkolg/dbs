                        <Insets left="10.0"/>
                     </VBox.margin></TextField>
               </children></VBox>
            <VBox prefHeight="75.0" prefWidth="300.0">
               <children>
                  <Label alignment="CENTER"  prefHeight="25.0" prefWidth="300.0" text="Street*" textFill="#871414" />
