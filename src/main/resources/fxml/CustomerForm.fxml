<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.paint.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox id="customerFormVBox" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cz.cvut.fel.dbs.gui.CustomerFormController">
       <children>
      <HBox prefHeight="75.0" prefWidth="600.0" spacing="10.0">
         <children>
            <VBox prefHeight="75.0" prefWidth="300.0">
               <children>
                  <Label alignment="CENTER" prefHeight="25.0" prefWidth="300.0" text="Name*" textFill="#871414" />
                  <TextField fx:id="customerNameTextField" prefHeight="35.0" prefWidth="300.0">
                     <VBox.margin>
                        <Insets left="10.0"/>
                     </VBox.margin></TextField>
               </children></VBox>
            <VBox prefHeight="75.0" prefWidth="300.0">
               <children>
                  <Label alignment="CENTER" prefHeight="25.0" prefWidth="300.0" text="City*" textFill="#871414" />
