                        <Insets right="10.0"/>
                     </VBox.margin></TextField>
               </children></VBox>
         </children>
      </HBox>
      
      
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="600.0" spacing="50.0">
         <children>
            <Button fx:id="customerCancelButton" mnemonicParsing="false" prefHeight="60.0" prefWidth="100.0" text="Cancel" />
            <Button id="customerSaveButton" fx:id="customerSaveButton" mnemonicParsing="false" prefHeight="60.0" prefWidth="100.0" text="Save" />
         </children></HBox>
   </children>
</VBox>
